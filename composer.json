{
    "name": "phprest/phprest",
    "description": "PHP Rest Framework.",
    "keywords": ["phprest", "framework", "micro", "rest", "restful", "api"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Adam Balogh",
            "email": "baloghbh@gmail.com",
            "homepage": "http://code-chronicle.blogspot.com",
            "role": "Developer"
        },
        {
            "name": "Ayan Ozturk",
            "email": "ayanozturk@gmail.com",
            "homepage": "https://ayanozturk.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-json": "*",
        "league/booboo": "^1.1",
        "league/container": "^1.3",
        "league/event": "^2.2",
        "league/route": "^1.2.5",
        "monolog/monolog": "^1.12",
        "stack/builder": "^1.0",
        "symfony/console": "^3.4",
        "symfony/http-kernel": "^3.2",
        "willdurand/hateoas": "^3.8",
        "willdurand/negotiation": "^1.5"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-mockery": "^1.1",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "bin": ["bin/phprest"],
    "autoload": {
        "psr-4": {
            "Phprest\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phprest\\Stub\\": "stub/",
            "Phprest\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:RewardGateway/route.git"
        }
    ],
    "suggest": {
        "phprest/phprest-service-request-filter": "For filtering Entites.",
        "phprest/phprest-service-validator": "For validating Entities.",
        "phprest/phprest-service-orm": "For managing Relational Databases."
    },
    "config": {
        "sort-packages": true
    }
}
